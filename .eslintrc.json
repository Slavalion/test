{
    "extends": ["plugin:vue/vue3-strongly-recommended", "eslint:recommended", "prettier"],
    "rules": {
        "quotes": ["error", "single"],
        "semi": ["error", "never"],
        "indent": ["error", 4],
        "vue/multi-word-component-names": ["off"]
    },
    "globals": {
        "route": "readonly",
        "Ziggy": "readonly",
        "axios": "readonly",
        "ymaps": "readonly",
        "ym": "readonly"
    }
}
